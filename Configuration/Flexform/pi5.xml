<?xml version="1.0" encoding="utf-8" standalone="yes" ?>
<T3DataStructure>
    <sheets>
        <overview>
            <ROOT>
                <TCEforms>
                    <sheetTitle>Konfiguration</sheetTitle>
                </TCEforms>
                <type>array</type>
                <el>
                    
                    <settings.type>
                        <TCEforms>
                            <label>Slideelemente</label>
                            <config>
                                <type>select</type>
                                <items type="array">
                                    <numIndex index="0" type="array">
                                        <numIndex index="0">Project-ELements</numIndex>
                                        <numIndex index="1">0</numIndex>
                                    </numIndex>
                                    <numIndex index="1" type="array">
                                        <numIndex index="0">Slide-Elements</numIndex>
                                        <numIndex index="1">1</numIndex>
                                    </numIndex>
                                </items>
                            </config>
                        </TCEforms>
                    </settings.type>
                    
                    <settings.switchtitlesbyclick>
                        <TCEforms>
                            <label>Should the Titles NOT to be switched?</label>
                            <config>
                              <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.switchtitlesbyclick>
                    
                    <settings.cropnottext>
                        <TCEforms>
                            <label>Don't crop the text</label>
                            <config>
                              <type>check</type>
                            </config>
                        </TCEforms>
                    </settings.cropnottext>
                    
                </el>
            </ROOT>
        </overview>
    </sheets>
</T3DataStructure>
